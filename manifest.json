{
	"name": "Github Stars Organiser",
	"version": "0.1.0",
	"description": "A Google Chrome extension to organise your starred repositories on github",
	"author": "Benoît Grélard",
	"icons": {},
	"content_scripts": [
		{
			"matches": [
				"https://github.com/stars",
				"https://github.com/stars/",
				"https://github.com/stars?*"
			],
			"js": [
				"libs/utils.js",
				"libs/TagsStore.js",
				"libs/EventEmitter.js",
				"libs/Model.js",
				"libs/View.js",
				"models/Tags.js",
				"views/TagLineView.js",
				"views/TagSidebarView.js",
				"main.js"
			],
			"css": [
				"styles/TagLine.css",
				"styles/TagSidebar.css"
			]
		}
	],
	"permissions": [
		"storage"
	],
	"background": {
		"page": "bg.html"
	},
	"manifest_version": 2
}
