{
	"name": "Github Stars Organiser",
	"version": "1.0.0",
	"description": "A Google Chrome extension to organise your github starred repositories",
	"author": "Benoît Grélard",
	"icons": {},
	"content_scripts": [
		{
			"matches": [
				"https://github.com/stars*"
			],
			"js": [
				"libs/utils.js",
				"libs/EventEmitter.js",
				"libs/Model.js",
				"libs/View.js",
				"models/Tags.js",
				"views/TagLineView.js",
				"views/TagSidebarView.js",
				"main.js"
			],
			"css": [
				"styles/TagLine.css",
				"styles/TagSidebar.css"
			]
		}
	],
	"manifest_version": 2
}
